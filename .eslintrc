{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "linebreak-style": 0,
    "jsx-a11y/img-has-alt": 0,
    "react/prop-types": 0,
  }
}
